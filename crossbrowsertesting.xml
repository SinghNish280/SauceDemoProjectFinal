  </test> <!-- Test -->
  
  <test name="firefoxtest">
   <parameter name="os" value = "Windows"/>
  <parameter name="browser" value = "firefox"/>
    <classes>
      <class name="tests.LoginTest"/>
      <class name="tests.Multiple_Login"/>
      <class name="tests.HomeTest"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
